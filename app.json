{
  "expo": {
    "name": "xProject",
    "slug": "xProject",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "xproject-scheme",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dexter_rhys.xProject",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to allow users to take photos.",
        "NSPhotoLibraryUsageDescription": "This app accesses the photo library to let users choose photos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dexter_rhys.xProject",
      "permissions": ["CAMERA", "WRITE_EXTERNAL_STORAGE", "READ_EXTERNAL_STORAGE"]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bd6776eb-8798-4bfc-bf58-63a3ec02708c"
      }
    }
  }
}
